        function datevalidation() {
            var startdate = Date.parse($(".StartDate").val().toString("dd/MM/yyyy"));
            var enddate = Date.parse($(".EndDate").val().toString("dd/MM/yyyy"));
            var returnvalue = true;
            if ($(".StartDate").val() != "" && $(".EndDate").val() == "") {
                $("#enddateerrmsg")[0].innerHTML = "Please Select End Date.";
                $("#startdateerrmsg")[0].innerHTML = "";
                returnvalue = false;
            }
            else if (startdate > enddate) {
                $("#startdateerrmsg")[0].innerHTML = "Start Date must be less than End Date.";
                $("#enddateerrmsg")[0].innerHTML = "";
                returnvalue = false;
            }
            else if ($("#startdateerrmsg")[0].innerHTML == "") {
                $("#startdateerrmsg")[0].innerHTML = "";
                $("#enddateerrmsg")[0].innerHTML = "";
            }
            if ($(".StartDate").val() == "" && $(".EndDate").val() != "") {
                $("#startdateerrmsg")[0].innerHTML = "Please Select Start Date.";
                $("#enddateerrmsg")[0].innerHTML = "";
                returnvalue = false;
            }
            else if (enddate < startdate) {
                $("#enddateerrmsg")[0].innerHTML = "Start Date must be less than End Date.";
                $("#startdateerrmsg")[0].innerHTML = "";
                returnvalue = false;
            }
            else if ($("#enddateerrmsg")[0].innerHTML == "") {
                $("#startdateerrmsg")[0].innerHTML = "";
                $("#enddateerrmsg")[0].innerHTML = "";
            }
            return returnvalue;
        }

        $("#btncampaigninfo").click(function () {
