                            {
                                @Html.Partial("_WizardCreateOrEditGeographic", Model.CampaignProfileGeographicModel)
                            }
                        </div>

                        <button class="accordion" id="demographicaccordion">Demographic</button>
                        <div class="panel" id="DemographicPanel">
