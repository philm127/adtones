    $(document).ready(function () {
        $('.i-checks').iCheck({
            checkboxClass: 'icheckbox_square-green',
            radioClass: 'iradio_square-green',
        });
        $('.i-checks').keyup(function (e) {
            console.log('keyup called');
            var code = e.keyCode || e.which;
            if (code == '9') {
                var thi$ = $(this);
                if (thi$[0].parentNode.className == "icheckbox_square-green hover checked") {
                    thi$[0].parentNode.className = 'icheckbox_square-green hover checked';
                }
                else {
                    thi$[0].parentNode.className = 'icheckbox_square-green hover';
                }
            }
        });
        $('.i-checks').keydown(function (e) {
            console.log('keyup called');
            var code = e.keyCode || e.which;
            if (code == '9') {
                var thi$ = $(this);
                if (thi$[0].parentNode.className == "icheckbox_square-green hover checked") {
                    thi$[0].parentNode.className = 'icheckbox_square-green hover checked';
                }
                else {
                    thi$[0].parentNode.className = 'icheckbox_square-green';
                }
            }
        });
    });

