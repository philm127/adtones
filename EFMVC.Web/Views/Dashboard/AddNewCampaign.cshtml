        function budgetdecimalfunction() {
            $("#MonthlyBudget").keypress(function (event) {
                $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
                if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                    return false;
                }
                var monthlyBudget = $(this).val();
                if ((monthlyBudget.indexOf('.') != -1) &&
                    (monthlyBudget.substring(monthlyBudget.indexOf('.')).length > 2) &&
                    (event.which != 0 && event.which != 8) &&
                    ($(this)[0].selectionStart >= monthlyBudget.length - 2)) {
                    event.preventDefault();
                    return false;
                }
            });
            $("#WeeklyBudget").keypress(function (event) {
                $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
