                    event.preventDefault();
                    return false;
                }
                var maximumBid = $(this).val();
                if ((maximumBid.indexOf('.') != -1) &&
                    (maximumBid.substring(maximumBid.indexOf('.')).length > 2) &&
                    (event.which != 0 && event.which != 8) &&
                    ($(this)[0].selectionStart >= maximumBid.length - 2)) {
                    event.preventDefault();
                    return false;
                }
                else if (maximumBid.length >= 5) {
                    event.preventDefault();
                    return false;
                }
            });
