        $("#CampaignId").change(function () {
            $(".loader").show();
            var campaignId = $(this).val();
            if (campaignId == "")
                campaignId = 0;
            if (campaignId != 0) {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("GetCampaignDetails", "Dashboard")',
                    data: { campaignId: campaignId },
                    success: function (data) {
                        if (data != "Fail") {
                            var CampaignHTML = $(data).find("#CampaignName").val();
                            $("#CampaignName").val(CampaignHTML);
                            var CampaignDescriptionHTML = $(data).find("#CampaignDescription").val();
                            $("#CampaignDescription").val(CampaignDescriptionHTML);
                            $("#CountryId").html("");
                            var CountryHTML = $(data).find("#CountryId").html();
                            $("#CountryId").append(CountryHTML);
                            $(".loader").hide();
                        }
                        else {
                            $("#cerrror").html(data);
                            $('#dvcaminfoerror').css("display", "block");
                            $(".loader").hide();
                        }
                    }
                });
            }
            else {
                $("#CampaignName").val("");
                $("#CampaignDescription").val("");
                $('#CountryId').prop('selectedIndex', 0);
                $(".loader").hide();
            }
        });

        $("#CampaignName").change(function () {
            var CampaignId = $("#CampaignId")[0].value;
            var CampaignName = $("#CampaignName")[0].value;
            if (CampaignName != "") {
                if (CampaignName != "" && CampaignId != "") {
                    $("#CampaignId").removeAttr("disabled");
                    return true;
                }
                $("#CampaignId").attr('disabled', 'disabled');
                return true;
            }
            else {
                $("#CampaignId").removeAttr("disabled");
                return true;
            }
        });

        $("#close").click(function () {
            $("#cerrror").html("");
            $('#dvcaminfoerror').css("display", "none");
        });

        function campaignvalidation() {
            //var campaignId = $("#CampaignId")[0].value;
            var CampaignName = $("#CampaignName")[0].value;
            var campaignDescription = $("#CampaignDescription")[0].value;
            var CountryId = $("#CountryId")[0].value;
            var returnvalue = true;
            //if (campaignId == "") {
            //    $("#CampaignId")[0].nextElementSibling.innerHTML = "The Campaign field is required.";
            //    returnvalue = false;
            //}
            //else {
            //    $("#CampaignId")[0].nextElementSibling.innerHTML = "";
            //}
            if (CampaignName == "") {
                $("#CampaignName")[0].nextElementSibling.innerHTML = "The Campaign Name field is required.";
                returnvalue = false;
            }
            else {
                $("#CampaignName")[0].nextElementSibling.innerHTML = "";
            }
            if (campaignDescription.length > 300) {
                //$("#CampaignDescription")[0].nextElementSibling.innerHTML = "The Campaign Description field cannot be greater than 300.";
                $("#CampaignDescription")[0].nextElementSibling.innerHTML = "The Campaign Description field cannot be more than 300 characters.";
                returnvalue = false;
            }
            else {
                $("#CampaignDescription")[0].nextElementSibling.innerHTML = "";
            }
            //if (campaignDescription == "") {
            //    $("#CampaignDescription")[0].nextElementSibling.innerHTML = "The Campaign Description field is required.";
            //    returnvalue = false;
            //}
            //else if (campaignDescription.length > 300) {
            //    //$("#CampaignDescription")[0].nextElementSibling.innerHTML = "The Campaign Description field cannot be greater than 300.";
            //    $("#CampaignDescription")[0].nextElementSibling.innerHTML = "The Campaign Description field cannot be more than 300 characters.";
            //    returnvalue = false;
            //}
            //else {
            //    $("#CampaignDescription")[0].nextElementSibling.innerHTML = "";
            //}
            if (CountryId == "") {
                $("#CountryId")[0].nextElementSibling.innerHTML = "The Country field is required.";
                returnvalue = false;
            }
            else {
                $("#CountryId")[0].nextElementSibling.innerHTML = "";
            }
            return returnvalue;
        }

        $("#campaignsave").click(function () {
            $(".loader").show();
            var campaignvalida = campaignvalidation();
            if (campaignvalida == true) {
                var campaignName = $("#CampaignName").val();
                var campaignDescription = $("#CampaignDescription").val();
                var campaignId = $("#CampaignId").val();
                var phoneticAlphabet = $("#PhoneticAlphabet").val();
                var countryId = $("#CountryId").val();
                var clientCheck = $(".checkbox")[0].checked;
                if (clientCheck == undefined)
                    clientCheck = false;
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("AddNewCampaignInfo", "Dashboard")',
                    data: { campaignId: campaignId, campaignName: campaignName, campaignDescription: campaignDescription, phoneticAlphabet: phoneticAlphabet, countryId: countryId, clientCheck: clientCheck },
                    success: function (data) {
                        if (data == "Exists") {
                            campaignName = campaignName + " already exists."
                            $("#cerrror").html(campaignName);
                            $('#dvcaminfoerror').css("display", "block");
                            $("#dvcaminfosuc").hide();
                            $(".loader").hide();
                        }
                        else if (data == "fail") {
                            data = data + " already exists."
                            $("#cerrror").html(data);
                            $('#dvcaminfoerror').css("display", "block");
                            $("#dvcaminfosuc").hide();
                            $(".loader").hide();
                        }
                        else if (data == "success") {
                            var successmsg = "Campaign " + campaignName + " added successfully.";
                            $("#csuceess").html(successmsg);
                            $("#dvcaminfosuc").css("display", "block");
                            $("#dvcaminfoerror").hide();
                            $("#CampaignOtherInfo").show();
                            $(".loader").hide();
                        }
                    }
                });
            }
            else {
                $(".loader").hide();
                return false;
            }
        });

        $("#CountryId").change(function () {
            $(".loader").show();
            var id = $(this).val();
            var label = "country";
            if (id == "") {
                $("#errormsg").html("");
                $("#CurrencyId").val("");
                $("#lblmonthlybudget").html("");
                $("#lblweeklybudget").html("");
                $("#lbldailybudget").html("");
                $("#lblhourlybudget").html("");
                $("#lblmaxbid").html("");
                $(".loader").hide();
            }
            else {
                $.post('@Url.Action("GetCurrencyCode", "Dashboard")', { id: id, label: label }, function (data) {
                    if (data.data = "success") {
                        $("#errormsg").html("");
                        var countrySymbole = data.value;
                        $("#CurrencyId").val(data.value1);
                        $("#lblmonthlybudget").html("(" + countrySymbole + ")");
                        $("#lblweeklybudget").html("(" + countrySymbole + ")");
                        $("#lbldailybudget").html("(" + countrySymbole + ")");
                        $("#lblhourlybudget").html("(" + countrySymbole + ")");
                        $("#lblmaxbid").html("(" + countrySymbole + ")");
                        $(".loader").hide();
                    }
                    else if (data.data = "fail") {
                        $(".loader").hide();
                    }
                });
            }
            var countryId = id == "" ? 0 : id;
            $.ajax({
                url: '@Url.Action("GetOperatorList", "Dashboard")',
                data: { countryId: countryId },
                success: function (data) {
                    var items = "<option value=''>--Select Operator--</option>";
                    if (data == "False") {
                        $('#OperatorId').html(items);
                    }
                    else {
                        $.each(data, function (i, us) {
                            items += "<option value='" + us.Value + "'>" + us.Text + "</option>";
                        });
                        $('#OperatorId').html(items);
                    }
                }
            });
        });

