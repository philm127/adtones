                    event.preventDefault();
                    return false;
                }
                var weeklyBudget = $(this).val();
                if ((weeklyBudget.indexOf('.') != -1) &&
                    (weeklyBudget.substring(weeklyBudget.indexOf('.')).length > 2) &&
                    (event.which != 0 && event.which != 8) &&
                    ($(this)[0].selectionStart >= weeklyBudget.length - 2)) {
                    event.preventDefault();
                    return false;
                }
            });
            $("#DailyBudget").keypress(function (event) {
                $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
                if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                    return false;
                }
                var dailyBudget = $(this).val();
                if ((dailyBudget.indexOf('.') != -1) &&
                    (dailyBudget.substring(dailyBudget.indexOf('.')).length > 2) &&
                    (event.which != 0 && event.which != 8) &&
                    ($(this)[0].selectionStart >= dailyBudget.length - 2)) {
                    event.preventDefault();
                    return false;
                }
            });
            $("#HourlyBudget").keypress(function (event) {
                $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
                if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                    return false;
                }
                var hourlyBudget = $(this).val();
                if ((hourlyBudget.indexOf('.') != -1) &&
                    (hourlyBudget.substring(hourlyBudget.indexOf('.')).length > 2) &&
                    (event.which != 0 && event.which != 8) &&
                    ($(this)[0].selectionStart >= hourlyBudget.length - 2)) {
                    event.preventDefault();
                    return false;
                }
            });
            $("#MaximumBid").keypress(function (event) {
                //var maximumbidcompare = $("#MaximumBid")[0].value;
                $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
