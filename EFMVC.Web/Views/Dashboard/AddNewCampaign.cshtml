@using EFMVC.Web.ViewModels;
@model NewCampaignProfileFormModel

@{
    ViewBag.Title = "AddCampaign";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    a.disabled {
        pointer-events: none;
        cursor: default;
        border: 1px solid white;
        border-radius: 10px;
    }
</style>

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="spiner-example loader" style="display:none;">
        <div class="sk-spinner sk-spinner-three-bounce">
            <div class="sk-bounce1"></div>
            <div class="sk-bounce2"></div>
            <div class="sk-bounce3"></div>
        </div>
    </div>
    <div class="col-sm-4">
        @*<h2>Add Campaign</h2>*@
        <h2>Campaign Wizard</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Dashboard")">Dashboard</a>
            </li>
            <li class="active" id="breadcrumbmenu">
                <strong>Campaigns</strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeIn" style="padding-top:0px;">
    <div class="p-w-md m-t-sm">
        <div class="row">
            <div class="step-content campaingactive">
                <div>
                    <div class="cols-lg-12">
                        <ul class="navigate">
                            <li><a class="disabled" id="campaigninformation">Campaign Information</a></li>
                            <li><a class="disabled" id="clientinformation">Client Information</a></li>
                            <li><a class="disabled" id="budgetmanagement">Budget Management</a></li>
                            <li><a class="disabled" id="advertinformation">Advert Information</a></li>
                            <li><a class="disabled" id="smsandemail">SMS and email</a></li>
                            <li><a class="disabled" id="adsprofilemapping">Ads Profile Mapping</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">&nbsp;</div>
            <div class="wrapper-content animated fadeInRight">
                <div class="row" id="campaigndiv">
                    @Html.Partial("_AddNewCampaignInfo", Model)
                </div>
                <div class="row" id="clientdiv" style="display:none;">
                    @Html.Partial("_AddNewClientInfo", Model.newClientFormModel)
                </div>
                <div class="row" id="budgetdiv" style="display:none;">
                    @Html.Partial("_AddNewBudgetInfo", Model.newBudgetFormModel)
                </div>
                <div class="row" id="advertdiv" style="display:none;">
                    @Html.Partial("_AddNewAdvertInfo", Model.newAdvertFormModel)
                </div>
                <div class="row" id="campaigndatesandinteractiondiv" style="display:none;">
                    @Html.Partial("_AddNewCampaignDateandInteraction", Model.newCampaignDateandInteraction)
                </div>
                <div class="row" id="adprofilemappingdiv" style="display:none;">
                    @Html.Partial("_AddNewAdProfileMappingInfo", Model.newAdProfileMappingFormModel)
                </div>
            </div>
        </div>
    </div>
</div>

