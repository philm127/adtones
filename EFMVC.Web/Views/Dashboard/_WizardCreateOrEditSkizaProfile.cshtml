                    <div class="form-group">

                        <div class="row" style="padding-top:15px;">
                            @if (Model.HustlersQuestion.Count() > 0)
                            {
                                if (Model.Hustlers_AdType != null || Model.Hustlers_AdType != "")
                                {
                                    <h3 class="col-lg-4 col-lg-wth">@Html.LabelFor(model => model.Hustlers_AdType)</h3>
                                }
                            }
                            @if (Model.YouthQuestion.Count() > 0)
                            {
                                if (Model.Youth_AdType != null || Model.Youth_AdType != "")
                                {
                                    <h3 class="col-lg-4 col-lg-wth">@Html.LabelFor(model => model.Youth_AdType)</h3>
                                }
                            }
                            @if (Model.DiscerningProfessionalsQuestion.Count() > 0)
                            {
                                if (Model.DiscerningProfessionals_AdType != null || Model.DiscerningProfessionals_AdType != "")
                                {
                                    <h3 class="col-lg-4 col-lg-wth">@Html.LabelFor(model => model.DiscerningProfessionals_AdType)</h3>
                                }
                            }
                            </div>
                        <div class="row">
                            @if (Model.HustlersQuestion.Count() > 0)
                            {
                                <div class="col-lg-4 col-lg-wth" style="line-height:28px;">
                                    @foreach (var item in Model.HustlersQuestion)
                                    {
                                        <div class="radio i-checks"><label>@Html.RadioButtonFor(model => model.Hustlers_AdType, item.QuestionValue)@item.QuestionName</label></div>
                                    }
                                </div>
                            }
                            @if (Model.YouthQuestion.Count() > 0)
                            {
                                <div class="col-lg-4 col-lg-wth" style="line-height:28px;">
                                    @foreach (var item in Model.YouthQuestion)
                                    {
                                        <div class="radio i-checks"><label>@Html.RadioButtonFor(model => model.Youth_AdType, item.QuestionValue)@item.QuestionName</label></div>
                                    }
                                </div>
                            }
                            @if (Model.DiscerningProfessionalsQuestion.Count() > 0)
                            {
                                <div class="col-lg-4 col-lg-wth" style="line-height:28px;">
                                    @foreach (var item in Model.DiscerningProfessionalsQuestion)
                                    {
                                        <div class="radio i-checks"><label>@Html.RadioButtonFor(model => model.DiscerningProfessionals_AdType, item.QuestionValue)@item.QuestionName</label></div>
                                    }
                                </div>
                            }
                        </div>

                        <div class="row" style="padding-top:15px;">
                            @if (Model.MassQuestion.Count() > 0)
                            {
                                if (Model.Mass_AdType != null || Model.Mass_AdType != "")
                                {
                                    <h3 class="col-lg-4 col-lg-wth">@Html.LabelFor(model => model.Mass_AdType)</h3>
                                }
                            }
                            </div>
                        <div class="row">
                            @if (Model.MassQuestion.Count() > 0)
                            {
                                <div class="col-lg-4 col-lg-wth" style="line-height:28px;">
                                    @foreach (var item in Model.MassQuestion)
                                    {
                                        <div class="radio i-checks"><label>@Html.RadioButtonFor(model => model.Mass_AdType, item.QuestionValue)@item.QuestionName</label></div>
                                    }
                                </div>
                            }
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-1">
                            <button class="btn btn-blue" type="submit">Save</button>

                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    var progressbar = function (data) {
        $(".spiner-example").show();
    };
    var getresultskizaprofile = function (data) {
        $(".spiner-example").hide();
        if (data == "success") {
            $("#dvskizasuc").show();
            $("#dvskizaerror").hide();
        }
        else {
            if (data == "notauthorise") {
                window.location.href = '@Url.Action("Index","Landing", new { Area = "" })';
            }
            $("#cadverterrror").html(data);
            $("#dvskizasuc").hide();
            $("#dvskizaerror").show();
        }
    };
</script>
