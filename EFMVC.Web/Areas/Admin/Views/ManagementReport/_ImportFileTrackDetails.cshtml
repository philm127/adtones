@using EFMVC.Model.Entities;
@model List<ImportFileTrack>
<div class="row">
    @if (TempData["status"] != null)
    {
        <div class="alert alert-success alert-dismissable">
            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
            @TempData["status"]
        </div>
    }
</div>
<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover dataTables-operator">
        <thead>
            <tr>               
                <th>Num Of TextFile</th>  
                <th>Num Of TextLine</th>
                <th>Num Of Update To Audit</th>
                <th>Num Of Play</th>
                <th>Num Of SMS</th>
                <th>Num Of Email</th>
                <th>Num Of User</th>
                <th>Num Of Removed User</th>
                @*<th>Num Of New User</th>
                <th>Num Of Live Campaign</th>*@
                <th>Operator Name</th>   
                <th>Added Date</th>      
                    
            </tr>
        </thead>
        <tbody>
            @foreach (var result in Model)
            {
                <tr class="gradeX">
                    <td>@result.NumOfTextFile</td>        
                    <td>@result.NumOfTextLine</td>  
                    <td>@result.NumOfUpdateToAudit</td> 
                    <td>@result.NumOfPlay</td> 
                    <td>@result.NumOfSMS</td> 
                    <td>@result.NumOfEmail</td> 
                    <td>@result.NumOfUser</td> 
                    <td>@result.NumOfRemovedUser</td> 
                    @*<td>@result.NumOfNewUser</td>
                    <td>@result.NumOfLiveCampaign</td>*@ 
                    <td>@result.Operator.OperatorName</td>  
                    <td>@result.AddedDate</td>     
                </tr>
            }
        </tbody>
    </table>
</div>

