@using EFMVC.Web.Areas.Admin.Models;
@model List<SystemConfigResult>
<div class="row">
    @if (TempData["success"] != null)
    {
        <div class="alert alert-success alert-dismissable">
            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
            @TempData["success"]
        </div>
    }
</div>
<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover dataTables-systemconfig">
        <thead>
            <tr>
                <th width="10%">System Config Key</th>
                <th width="60%">System Config Value</th>
                <th width="10%">Created Date</th>
               
                <th width="10%">System Config Type</th>
                <th width="10%">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (SystemConfigResult result in Model)
            {
                <tr class="gradeX">
                    <td><a href='@Url.Action("ConfigDetails", "SystemConfig", new { @id = result.SystemConfigId })'>@result.SystemConfigKey</a></td>
                    <td>@result.SystemConfigValue</td>
                    <td>@result.CreatedDateTime</td>
                    <td>@result.SystemConfigType</td>
                    <td>
                        <a class="btn btn-danger btn-xs" href='@Url.Action("ConfigDetails", "SystemConfig", new { @id = result.SystemConfigId })'>Update</a>
                        <a class="btn btn-danger btn-xs" href='@Url.Action("Delete","SystemConfig",new {@id=result.SystemConfigId })'>Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
