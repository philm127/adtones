@model EFMVC.Web.ViewModels.UserProfileMobileFormModel
@*<script src="~/Scripts/jquery-1.5.1.min.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.validate.min.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>*@

@Html.HiddenFor(model => model.UserProfileMobileId)
@Html.HiddenFor(model => model.UserProfileId)

<div class="row">
    <div class="large-6 columns">
        <h4>Mobile</h4>
        <h6 class="font14">Please provide the information requested below.</h6>
    </div>
</div>

<div class="clr10"></div>

<div class="row">
    <div class="large-6 columns">
        <div class="panel">
            <div class="editor-field">
                @Html.LabelFor(model => model.ContractType)
            </div>
            <div class="clr10"></div>
            <div class="editor-field">
                @Html.DropDownListFor(model => model.ContractType, Model.ContractTypeList)
                @Html.ValidationMessageFor(model => model.ContractType)
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <div class="panel">
            <div class="editor-field">
                @Html.LabelFor(model => model.Spend)
            </div>
            <div class="clr10"></div>
            <div class="editor-field">
                @Html.DropDownListFor(model => model.Spend, Model.SpendList)
                @Html.ValidationMessageFor(model => model.Spend)
            </div>
        </div>
    </div>
</div>